<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.collections.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.collections.FXCollections?>

<VBox spacing="8.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
      <HBox spacing="8.0">
         <children>
            <TextField fx:id="filepathField" prefColumnCount="25" HBox.hgrow="ALWAYS" />
            <Button fx:id="browseButton" mnemonicParsing="false" onAction="#onBrowseButtonAction" text="Browse" />
         </children>
      </HBox>
      <Separator>
         <padding>
            <Insets bottom="3.0" top="3.0" />
         </padding>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Separator>
      <HBox>
         <children>
            <Label text="Delimiter:">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Label>
            <Region prefWidth="60.0" HBox.hgrow="SOMETIMES" />
            <TextField fx:id="delimiterField" prefColumnCount="4" />
            <Region HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <HBox layoutX="15.0" layoutY="65.0">
         <children>
            <Label text="Header:" />
            <Region prefWidth="90.0" HBox.hgrow="SOMETIMES" />
            <CheckBox fx:id="headerBox" mnemonicParsing="false" />
            <Region HBox.hgrow="ALWAYS" />
         </children>
      </HBox><HBox alignment="CENTER_RIGHT">
        <children><Button fx:id="okButton" defaultButton="true" mnemonicParsing="false" onAction="#onOkButtonAction" text="OK" />
        </children>
        <opaqueInsets>
            <Insets />
        </opaqueInsets>
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </padding></HBox>
    </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding></VBox>
