<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox spacing="8.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
      <HBox spacing="8.0">
         <children>
            <TextField fx:id="filepathField" onAction="#onTextfieldAction" prefColumnCount="25" HBox.hgrow="ALWAYS" />
            <Button fx:id="browseButton" mnemonicParsing="false" onAction="#onBrowseButtonAction" text="Browse" />
         </children>
      </HBox>
      <Separator>
         <padding>
            <Insets bottom="3.0" top="3.0" />
         </padding>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Separator>
      <HBox>
         <children>
            <Label prefWidth="80.0" text="Delimiter:">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <padding>
                  <Insets left="10.0" />
               </padding>
            </Label>
            <Region prefWidth="60.0" HBox.hgrow="SOMETIMES" />
            <TextField fx:id="delimiterField" onAction="#onTextfieldAction" prefColumnCount="2" />
            <Region HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <HBox>
         <children>
            <Label prefWidth="80.0" text="Decimal:">
               <padding>
                  <Insets left="10.0" />
               </padding>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Label>
            <Region prefWidth="60.0" HBox.hgrow="SOMETIMES" />
            <TextField fx:id="decimalField" onAction="#onTextfieldAction" prefColumnCount="2" />
            <Region HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <HBox layoutX="15.0" layoutY="65.0">
         <children>
            <Label prefWidth="80.0" text="Header:">
               <padding>
                  <Insets left="10.0" />
               </padding></Label>
            <Region prefWidth="70.0" HBox.hgrow="SOMETIMES" />
            <CheckBox fx:id="headerBox" mnemonicParsing="false" onAction="#onTextfieldAction" />
            <Region HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <HBox>
         <children>
            <Label fx:id="infoLabel" prefWidth="358.0" textFill="#fc0000" />
         </children>
      </HBox><HBox alignment="CENTER_RIGHT" spacing="8.0">
        <children><Button fx:id="okButton" defaultButton="true" mnemonicParsing="false" onAction="#onOkButtonAction" prefWidth="65.0" text="OK" />
            <Button fx:id="cancelButton" defaultButton="true" layoutX="330.0" layoutY="15.0" mnemonicParsing="false" onAction="#onCancelButtonAction" prefWidth="65.0" text="Cancel" />
        </children>
        <opaqueInsets>
            <Insets />
        </opaqueInsets></HBox>
    </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding></VBox>
